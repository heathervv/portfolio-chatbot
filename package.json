{
  "name": "portfolio-chatbot",
  "version": "2.2.0",
  "private": true,
  "dependencies": {
    "axios": "1.13.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-draggable": "4.5.0",
    "react-linkify": "1.0.0-alpha",
    "react-transition-group": "4.4.5",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "24.10.0",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "@vitejs/plugin-react-swc": "4.2.3",
    "eslint": "9.39.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "gh-pages": "6.3.0",
    "jiti": "2.6.1",
    "npm-run-all": "4.1.5",
    "sass": "1.97.3",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-svgr": "4.5.0"
  },
  "scripts": {
    "build-css": "sass src:src",
    "watch-css": "npm run build-css && sass --watch src:src",
    "start-js": "vite",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "vite build",
    "build": "npm-run-all build-css build-js",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "check:ts-only": "node scripts/check-ts-only.mjs",
    "check": "npm run check:ts-only && npm run typecheck && npm run lint",
    "serve": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "homepage": "https://heathervv.github.io"
}